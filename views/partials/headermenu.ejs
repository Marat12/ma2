<!DOCTYPE html>
<html>
    <head>
        <title>Водоподготовка</title>
        <link rel="stylesheet" href="/stylesheets/main.css">
        <link rel="stylesheet" href="/stylesheets/adminstyle.css">
        <meta name="viewport" content="width=device-width, initial-scale=1">
    </head>
    <body>
<div class="container-fluid">
<div id="catalog" class="row text-center" style="display: flex; flex-wrap: wrap">
  <div class="col-xs-12 col-md-3 p3">
    <p></p>
    <img class="card-img-top img-fluid" src="/pictures/voda.jpg" alt="Card image cap">
    <!--<h2>Каталог Товаров</h2>-->
    <div class="list-group-flush card">
      <strong><a href="/products/p/1"  class="list-group-item text title-hover-background">Весь ассортимент магазина</a></strong>
      <% categories.forEach(function(c) { %> 
      <strong><a class="list-group-item text title-hover-background" href="/products/<%= c.slug %>/s/1"><%= c.title %></a></strong>
      <% }); %>
    </div>
  </div>
  

